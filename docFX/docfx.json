{
    "metadata": [{
        "src": [{
            "files": [
                "**/Assembly-CSharp.csproj",
                "**/Assembly-CSharp-Editor.csproj"
            ],
            "src": "../"
        }],
        "dest": "api",
        "filter": "filterConfig.yml",
        "shouldSkipMarkup": false,
        "disableGitFeatures": false,
        "disableDefaultFilter": false,
        "force": true
    }],
    "build": {
        "content": [{
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [{
            "files": [
                "images/**"
            ]
        }],
        "overwrite": [{
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "**/docs/**"
            ]
        }],
        "dest": "../docs/",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "globalMetadata": {
            "_enableSearch": true
        },
        "sitemap": {
            "baseUrl": "https://watson70percent.github.io/SAMURAI-SOCCER"
        }
    }
}