    - m_characterName: 4
      m_text: "\u6975\u6771\u306E\u4EBA\u6C11\u3088\uFF01\u5354\u529B\u3057\u3066\u30A6\u30A9\u30C3\u30AB\u3092\u751F\u7523\u3057\u52B4\u50CD\u3092\u5171\u6709\u305B\u3088\uFF01"
    - m_characterName: 4
      m_text: "\u3055\u3082\u306A\u304F\u3070\u7279\u5225\u30B5\u30C3\u30AB\u30FC\u4F5C\u6226\u3092\u958B\u59CB\u3059\u308B\u3002"
    - m_characterName: 0
      m_text: "\u3046\u3049\u3063\u304B\u3092\u305F\u3060\u3067\u3088\u3053\u305B\u3068\u3044\u3075\u304B\u3002\u7B11\u6B62\u5343\u4E07\uFF01\u6570\u3005\u306E\u5217\u5F37\u3092\u6253\u3061\u7834\u3063\u305F\u6211\u304C\u56FD\u306E\u529B\u3092\u898B\u3088\uFF01"
  - m_leftCharacterName: 0
    m_rightCharacterName: 4
    m_conversationTexts:
    - m_characterName: 4
      m_text: "\u6DF1\u304F\u6211\u304C\u7956\u56FD\u3078\u62DB\u304D\u5165\u308C\u3088\u3046\u3067\u306F\u306A\u3044\u304B\u3002\n\u6211\u304C\u30ED\u30B7\u30A2\u3067\u306F\u6C37\u304C\u4EBA\u6C11\u3092\u6ED1\u3089\u305B\u308B\uFF01"
  - m_leftCharacterName: 0
    m_rightCharacterName: 4
    m_conversationTexts:
    - m_characterName: 4
      m_text: "\u306A\u304B\u306A\u304B\u3057\u3076\u3068\u3044\u3084\u3064\u3081\u3002\n\u6700\u7D42\u5175\u5668\"\u30AB\u30B9\u30D4\u6D77\u306E\u602A\u7269\"\u306E\u51FA\u6483\u3092\u8A31\u53EF\u3059\u308B\uFF01"
  - m_leftCharacterName: 0
    m_rightCharacterName: 4
    m_conversationTexts:
