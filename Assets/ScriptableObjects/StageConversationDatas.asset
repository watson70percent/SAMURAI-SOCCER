  - m_leftCharacterName: 0
    m_rightCharacterName: 1
    m_conversationTexts:
    - m_characterName: 1
      m_text: "\u65E5\u672C\u306E\u7686\u3055\u30FC\u3093\u3001\u30B3\u30F3\u30CB\u30C1\u30CF\u30C7\u30FC\u30B9"
    - m_characterName: 0
      m_text: "\u304A\u4E3B\u3001\u4F55\u8005\u3060\uFF01"
    - m_characterName: 1
      m_text: "\u30EF\u30BF\u30B7\u306F\u30D6\u30EA\u30C6\u30A3\u30C3\u30B7\u30E5\u306E\u30B3\u30AF\u30AA\u30FC\u30C7\u30FC\u30B9"
    - m_characterName: 1
      m_text: "\u65E5\u672C\u306B\u306F\u3053\u308C\u304B\u3089\u30EF\u30BF\u30B7\u306E\u56FD\u306E\u7D05\u8336\u3092\u5927\u91CF\u306B\u8F38\u5165\u3057\u3001\u6BCE\u65E5\u98F2\u3093\u3067\u3082\u3089\u3044\u30DE\u30FC\u30B9"
    - m_characterName: 0
      m_text: "\u305D\u3093\u306A\u6761\u7D04\u3001\u5230\u5E95\u53D7\u3051\u5165\u308C\u3089\u308C\u306C\u308F\uFF01"
    - m_characterName: 1
      m_text: "\u306A\u3089\u3070\u30B5\u30C3\u30AB\u30FC\u3092\u3059\u308B\u306E\u30C7\u30FC\u30B9"
    - m_characterName: 1
      m_text: "\u30B5\u30C3\u30AB\u30FC\u3067\u52DD\u3063\u305F\u65B9\u306E\u8A00\u3046\u3053\u3068\u3092\u805E\u304F\u306E\u30C7\u30FC\u30B9"
    - m_characterName: 1
      m_text: "(\u6700\u5F31\u306E\u65E5\u672C\u304C\u52DD\u3066\u308B\u308F\u3051\u306A\u3044\u306E\u30C7\u30FC\u30B9)"
    - m_characterName: 0
      m_text: "\u3044\u3044\u3060\u308D\u3046\u3002\u6211\u304C\u56FD\u306E\u771F\u306E\u5B9F\u529B\u3092\u601D\u3044\u77E5\u308B\u304C\u826F\u3044\uFF01"
  - m_leftCharacterName: 0
    m_rightCharacterName: 1
    m_conversationTexts:
    - m_characterName: 1
      m_text: "\u65E5\u672C\u306B\u8CA0\u3051\u308B\u306A\u3093\u3066\u3042\u308A\u3048\u306A\u3044\u306E\u30C7\u30FC\u30B9\uFF01"
    - m_characterName: 0
      m_text: "\u30B3\u30AF\u30AA\u30FC\u3088\u3001\u6211\u304C\u56FD\u306E\u79D8\u7B56\u3001\u601D\u3044\u77E5\u3063\u305F\u304B"
    - m_characterName: 1
      m_text: "...\u30DE\u30C0\u306A\u306E\u30C7\u30FC\u30B9"
    - m_characterName: 1
      m_text: "\u3053\u3061\u3089\u3082\u79D8\u7B56\u3092\u7528\u610F\u3059\u308B\u306E\u30C7\u30FC\u30B9"
  - m_leftCharacterName: 0
    m_rightCharacterName: 1
    m_conversationTexts:
    - m_characterName: 0
      m_text: "\u307E\u305F\u3057\u3066\u3082\u6211\u304C\u56FD\u306E\u52DD\u5229\u3060"
    - m_characterName: 1
      m_text: "\u30DE\u30C0\u7D42\u308F\u3063\u3066\u3044\u306A\u3044\u306E\u30C7\u30FC\u30B9\uFF01"
    - m_characterName: 1
      m_text: "\u5473\u564C\u3092\u30DE\u30FC\u30DE\u30A4\u30C8\u306B\u5909\u3048\u308B\u306E\u30C7\u30FC\u30B9\uFF01"
    - m_characterName: 1
      m_text: "...\u672C\u7269\u306E\u300C\u5175\u5668\u300D\u3092\u98DF\u3089\u308F\u305B\u3066\u3084\u308B\u306E\u30C7\u30FC\u30B9"
  - m_leftCharacterName: 0
    m_rightCharacterName: 1
    m_conversationTexts:
